# Add real values then rename to .env
NCBI_EMAIL=your_email@example.com
NCBI_API_KEY=optional_key_for_higher_rate
SEMANTIC_SCHOLAR_KEY=optional
CUDA_VISIBLE_DEVICES=0

# Data paths
DATA_DIR=./data
RAW_DATA_DIR=./data/raw
PROCESSED_DATA_DIR=./data/processed
GRAPHS_DATA_DIR=./data/graphs

# Processing settings
MAX_WORKERS=4
BATCH_SIZE=1000
CACHE_ENABLED=true

# NLP model settings
NER_MODEL=en_ner_bc5cdr_md
RE_MODEL=dmis-lab/biobert-base-cased-v1.1
DEVICE=auto  # auto, cpu, cuda

# Graph settings
MIN_EDGE_WEIGHT=1
MAX_NODES=50000
COMMUNITY_RESOLUTION=1.0
